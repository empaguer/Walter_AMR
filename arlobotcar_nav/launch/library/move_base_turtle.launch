<launch>
  <!-- Arguments -->
  <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
  <arg name="cmd_vel_topic" default="/cmd_vel" />
  <arg name="odom_topic" default="odom" />
  <arg name="move_forward_only" default="false"/>

  <!-- move_base -->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">

    <rosparam file="$(find arlobotcar_nav)/config/common_costmap.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find arlobotcar_nav)/config/common_costmap.yaml" command="load" ns="local_costmap" />
    
    <rosparam file="$(find arlobotcar_nav)/config/local_costmap.yaml" command="load"/>
    <rosparam file="$(find arlobotcar_nav)/config/global_costmap.yaml" command="load"/> 
        <rosparam file="$(find arlobotcar_nav)/config/planner.yaml" command="load" />
  </node>
</launch>
